{
  "name": "e-commerce",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@stripe/react-stripe-js": "^1.7.2",
    "@stripe/stripe-js": "^1.29.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/styled-components": "^5.1.25",
    "babel-plugin-macros": "^3.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "firebase": "^9.6.10",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.5",
    "sass": "^1.49.9",
    "stripe": "^9.1.0",
    "styled-components": "^5.3.5",
    "typed-redux-saga": "^1.4.0",
    "typescript": "^4.6.4",
    "ts-jest": "^29.1.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "preinstall": "cd $(pwd)",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|svg|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./__tests__/"
    },
    "transform": {
      "^.+\\.[t|j]sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!swiper|ssr-window|dom7).*/"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "modulePaths": [
      "src",
      "__test__"
    ],
    "testEnvironment": "jest-environment-jsdom"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/redux-logger": "^3.0.9"
  }
}
